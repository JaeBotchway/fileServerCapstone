    <script type = "text/javascript">
    
function total(numb)   //declare function called total
{
 return(numb * 5);       //value for total function is numb| calculate num  by multiplying  5
}
document.write(total(5));  //declare value of numb as 5|output is numb(5) multiply 5

function add(number1)       //declare function add
{
return(50 + number1);       //value for function add is number1| calculate number1 by adding 50
}
document.write("number is" + " "+ add(5)); //call the value of number1 as 5 | output the addition of 50+number1

    </script>
    
